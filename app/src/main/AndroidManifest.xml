<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.poodlemoth.todoly">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Todoly">
        <activity android:name=".activity_priority" />
        <activity android:name=".createTaskActivity" />
        <activity android:name=".SecondActivity" />
        <activity android:name=".CalendarActivity" />
        <activity android:name=".CalendarMainActivity" />
        <activity android:name=".activity_pomodoro"/>
        <activity
            android:name=".DefaultActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>